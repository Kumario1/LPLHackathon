# OpenClaw / Clawdbot Server Configuration
# Copy to openclaw/.env and update

# Backend URL (Transition OS)
BACKEND_URL=http://localhost:8000

# Optional API key if backend enforces auth
# BACKEND_API_KEY=your-secret-token

# CORS origins (comma-separated) or "*" for all
OPENCLAW_ALLOWED_ORIGINS=http://localhost:5173

# Server ports (optional)
OPENCLAW_HOST=0.0.0.0
OPENCLAW_PORT=18789
CLAWDBOT_HOST=0.0.0.0
CLAWDBOT_PORT=8080

LOG_LEVEL=INFO
